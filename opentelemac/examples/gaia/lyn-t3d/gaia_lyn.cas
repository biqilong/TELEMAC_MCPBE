/-----------------------------------------------------------------------
/ Gaia steering file
/-----------------------------------------------------------------------
BOUNDARY CONDITIONS FILE = geo_lyn.cli
GEOMETRY FILE            = geo_lyn.slf
RESULTS FILE             = gaia_lyn.slf
DEBUGGER = 0
/-----------------------------------------------------------------------
/ Output
/-----------------------------------------------------------------------
VARIABLES FOR GRAPHIC PRINTOUTS = 'U,V,S,E,H,B,M,TOB,*ES,KS'
MASS-BALANCE                    = YES
/-----------------------------------------------------------------------
/ Numerical parmeters
/-----------------------------------------------------------------------
FINITE VOLUMES                          = NO
/-----------------------------------------------------------------------
/ Physical parameters
/-----------------------------------------------------------------------
CLASSES TYPE OF SEDIMENT = NCO
BED LOAD FOR ALL SANDS                   = NO
SUSPENSION FOR ALL SANDS                 = YES
SUSPENSION TRANSPORT FORMULA FOR ALL SANDS = 1
/original settling velocities: 0.016
CLASSES SETTLING VELOCITIES = 0.016
/CLASSES SETTLING VELOCITIES = 0.0
CLASSES SEDIMENT DENSITY = 1600
SKIN FRICTION CORRECTION                = 1
FORMULA FOR SLOPE EFFECT                =0
FORMULA FOR DEVIATION                   =0
SEDIMENT SLIDE                          = NO
/-----------------------------------------------------------------------
/ Numerical parameters for suspension
/-----------------------------------------------------------------------
MAXIMUM NUMBER OF ITERATIONS FOR SOLVER FOR SUSPENSION = 200
/ these keywords exist in the dictionary but are not linked to the solution
/ of suspension in 3d (so these parameters are given in the t3d steering file
/ e.g. PRECONDITIONING FOR DIFFUSION OF TRACERS
/PRECONDITIONING FOR DIFFUSION OF SUSPENSION    = 34
/SOLVER FOR DIFFUSION OF SUSPENSION             = 7
/SOLVER OPTION FOR DIFFUSION OF SUSPENSION      = 5
